{
  "includes": [".eslintrc.json", "src/**/*.ts", "src/**/*.tsx"],
  "compilerOptions": {
    "strict": true,
    "module": "ESNext",
    "target": "ESNext",
    "moduleResolution": "bundler",

    "esModuleInterop": true,
    "sourceMap": false,
    // "rootDir": "extension/src",
    "outDir": "dist/js",
    "noEmitOnError": true,
    "jsx": "react",
    "typeRoots": ["node_modules/@types"]
  }
}
